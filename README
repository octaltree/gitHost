{
  "bitbucket": {
    "consumer": {
      "key": "*****",
      "secret": "*****"
    },
    "tokens": {
      "username": {
        "access_token": "****"
        "refresh_token": "****"
        "token_type": "****"
        "expires_in": "****"
      },
      "hoge": {
        "access_token": "****"
      },
      "foo": {
        "access_token": "****"
      }
    }
  },
  "github": {
    "consumer": {
      "key": "*****",
      "secret": "*****"
    },
    "tokens": {
    }
  },
  "gitlab": {
    "consumer": {
      "key": "*****",
      "secret": "*****"
    },
    "tokens": {
    }
  }
}
```
